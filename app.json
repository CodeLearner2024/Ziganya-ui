{
  "expo": {
    "name": "ziganya",
    "slug": "ziganya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Ziganya.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.eric.ziganyaApp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "61e75e92-bfba-4289-97e5-61ad7e9f23d8"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-font",
      "expo-router"
    ]
  }
}
